         <Style TargetType="{x:Type Button}">
            <Setter Property="Background" Value="{DynamicResource FlatButtonBackgroundBrush}"/>
            <Setter Property="BorderThickness" Value="0"/>
            <Setter Property="FontSize" Value="{DynamicResource FlatButtonFontSize}"/>
            <Setter Property="Foreground" Value="{DynamicResource FlatButtonForegroundBrush}"/>
            <Setter Property="Template">
               <Setter.Value>
                  <ControlTemplate TargetType="{x:Type Button}">
                     <Border
                        x:Name="Border"
                        Margin="0"
                        Background="{TemplateBinding Background}"
                        BorderBrush="{TemplateBinding BorderBrush}"
                        BorderThickness="{TemplateBinding BorderThickness}"
                        CornerRadius="5"
                        SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}">
                        <ContentPresenter
                           x:Name="ContentPresenter"
                           HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"
                           Margin="{TemplateBinding Padding}"
                           VerticalAlignment="{TemplateBinding VerticalContentAlignment}"
                           Content="{TemplateBinding Content}"
                           ContentTemplate="{TemplateBinding ContentTemplate}"/>
                     </Border>
                  </ControlTemplate>
               </Setter.Value>
            </Setter>
            <Style.Triggers>
               <Trigger Property="IsMouseOver" Value="True">
   <!-- Windows 11 Theme Dark Blue -->
                  <Setter Property="Background" Value="#0096D6"/>
               </Trigger>
               <Trigger Property="IsMouseOver" Value="False">
   <!-- Windows 11 Theme Blue -->
                  <Setter Property="Background" Value="#0067C0"/>
               </Trigger>
               <Trigger Property="IsPressed" Value="True">
                  <Setter Property="Background" Value="{DynamicResource FlatButtonPressedBackgroundBrush}"/>
                  <Setter Property="Foreground" Value="{DynamicResource FlatButtonPressedForegroundBrush}"/>
               </Trigger>
               <Trigger Property="IsEnabled" Value="False">
                  <Setter Property="Foreground" Value="{DynamicResource GrayBrush2}"/>
               </Trigger>
            </Style.Triggers>
         </Style>
         <Style TargetType="{x:Type ComboBox}">
            <Setter Property="FontFamily" Value="Segoe UI"/>
         </Style>
         <Style TargetType="{x:Type Label}">
            <Setter Property="FontFamily" Value="Segoe UI"/>
         </Style>
         <Style TargetType="{x:Type TextBox}">
            <Setter Property="FontFamily" Value="Segoe UI"/>
         </Style>
         <Style TargetType="{x:Type Window}">
            <Setter Property="FontFamily" Value="Segoe UI"/>
            <Setter Property="FontSize" Value="16"/>
            <Setter Property="Background" Value="Black"/>
            <Setter Property="Foreground" Value="Black"/>
         </Style>
      </ResourceDictionary>
   </Window.Resources>
   <Window.Background>
      <RadialGradientBrush Center="0.4,0.1" GradientOrigin="0.2,0.2" RadiusX="0.7" RadiusY="0.8">
         <RadialGradientBrush.RelativeTransform>
            <TransformGroup>
               <ScaleTransform CenterX="0.5" CenterY="0.5"/>
               <SkewTransform CenterX="0.5" CenterY="0.5"/>
               <RotateTransform Angle="-40.601" CenterX="0.5" CenterY="0.5"/>
               <TranslateTransform/>
            </TransformGroup>
         </RadialGradientBrush.RelativeTransform>
         <GradientStop Color="White"/>
         <GradientStop Offset="0.056" Color="#FFF9FFFE"/>
         <GradientStop Offset="0.776" Color="#FFF8FEFF"/>
         <GradientStop Offset="0.264" Color="#FFF4FAFF"/>
         <GradientStop Offset="0.506" Color="White"/>
         <GradientStop Offset="1" Color="AliceBlue"/>
      </RadialGradientBrush>
   </Window.Background>
   <Grid Name="RootGrid">
      <StackPanel Margin="0,10,0,10" Orientation="Vertical">
         <StackPanel Margin="0,10,0,10" Orientation="Vertical">
            <StackPanel HorizontalAlignment="Right" Margin="20,0,0,0" Orientation="Horizontal">
               <Image Height="45" Source="{StaticResource Microsoft_Azure}"/>
               <Label
                  Margin="5 0 10 0 "
                  Content="AzOSDPAD"
                  FontSize="25"
                  FontWeight="Bold"
                  Foreground="#01786A">
               </Label>
            </StackPanel>
            <StackPanel HorizontalAlignment="Left" Margin="10,0,0,0" Orientation="Horizontal">
               <Image Height="30" Source="{StaticResource Storage_Accounts}"/>
               <StackPanel Orientation="Horizontal">
                  <Label
                     Margin="5"
                     Content="Storage Account :"
                     FontSize="18"
                     FontWeight="Bold"
                     Foreground="#0096D6"
                     Padding="2">
                  </Label>
                  <Label
                     Name="StorageAccountName"
                     Width="165"
                     Margin="0 10 0 0 "
                     Content=""
                     FontSize="13"
                     FontWeight="Bold"
                     Padding="2">
                  </Label>
               </StackPanel>
            </StackPanel>
            <StackPanel HorizontalAlignment="Left" Margin="10,0,0,0" Orientation="Horizontal">
               <Image Height="30" Source="{StaticResource Resource_Groups}"/>
               <StackPanel Orientation="Horizontal">
                  <Label
                     Margin="5"
                     Content="Resource Group :"
                     FontSize="18"
                     FontWeight="Bold"
                     Foreground="#0096D6"
                     Padding="2">
                  </Label>
                  <Label
                     Name="ResourceGroup"
                     Width="165"
                     Margin="0 10 0 0 "
                     Content=""
                     FontSize="13"
                     FontWeight="Bold"
                     Padding="2">
                  </Label>
               </StackPanel>
            </StackPanel>
            <Line
               Width="800"
               Margin="0 10 0 10"
               Stretch="Uniform"
               Stroke="Gainsboro"
               StrokeThickness="1"
               X1="0"
               X2="2"
               Y1="0"
               Y2="0">
            </Line>
         </StackPanel>
         <StackPanel
            x:Name="StackElements"
            HorizontalAlignment="Center"
            Margin="0,25,0,0"
            Orientation="Horizontal">
            <Border
               Width="300"
               Height="215"
               Margin="-10 0 90 0 "
               BorderBrush="{DynamicResource MahApps.Brushes.Accent}"
               BorderThickness="3">
               <StackPanel>
                  <Label
                     HorizontalAlignment="Stretch"
                     Margin="5 0 0 5"
                     Content="Containters :"
                     FontSize="15"
                     FontWeight="Bold"
                     Foreground="#0096D6"
                     Padding="2">
                  </Label>
                  <TreeView Name="TreeView" Width="290" Height="178">
                     <TreeView.Resources>
                        <Style TargetType="{x:Type TreeViewItem}">
                           <Setter Property="HeaderTemplate">
                              <Setter.Value>
                                 <HierarchicalDataTemplate>
                                    <StackPanel Orientation="Horizontal">
                                       <Image
                                          Name="img"
                                          Width="25"
                                          Height="25"
                                          Source="{Binding RelativeSource={RelativeSource Mode=FindAncestor, AncestorType={x:Type TreeViewItem}}, Path=Tag, Converter={sys:ObjectTagToImageConverter}}"
                                          Stretch="Fill"/>
                                       <TextBlock Margin="5,0" Text="{Binding}"/>
                                    </StackPanel>
                                 </HierarchicalDataTemplate>
                              </Setter.Value>
                           </Setter>
                        </Style>
                     </TreeView.Resources>
                  </TreeView>
               </StackPanel>
            </Border>
            <Border
               Width="300"
               Height="210"
               Margin="5,0,0,0"
               BorderBrush="{DynamicResource MahApps.Brushes.Accent}"
               BorderThickness="3">
               <StackPanel HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Orientation="Vertical">
                  <StackPanel Background="{DynamicResource MahApps.Brushes.Accent}" Orientation="Horizontal">
                     <Label
                        HorizontalAlignment="Stretch"
                        Margin="7 0 0 5"
                        Content="Objects:"
                        FontSize="15"
                        FontWeight="Bold"
                        Foreground="#0096D6"
                        Padding="2"/>
                     <Label
                        Name="CObjects"
                        HorizontalAlignment="Stretch"
                        Content="2"
                        FontWeight="Bold"
                        Foreground="#0096D6"
                        Padding="2"/>
                  </StackPanel>
                  <Grid
                     x:Name="Destination"
                     Height="200"
                     HorizontalAlignment="Stretch"
                     VerticalAlignment="Top"
                     Background="#FFFFFF">
                     <ScrollViewer HorizontalScrollBarVisibility="Disabled" VerticalScrollBarVisibility="Auto">
   <!-- LISTBOX -->
                        <ListBox
                           Name="ListBoxControl"
                           Width="290"
                           Height="175"
                           VerticalAlignment="Top"
                           HorizontalContentAlignment="Stretch">
                           <ListBox.ItemTemplate>
                              <DataTemplate>
                                 <StackPanel Orientation="Horizontal">
                                    <Image
                                       Name="img"
                                       Height="25"
                                       Margin=" 0 2 5 0 "
                                       Source="{StaticResource PowershellSCript}"
                                       Stretch="Fill"/>
                                    <TextBlock
                                       Width="280"
                                       Margin="5,0"
                                       Foreground="Black"
                                       Text="{Binding}"/>
                                 </StackPanel>
                              </DataTemplate>
                           </ListBox.ItemTemplate>
                           <ListBox.ContextMenu>
                              <ContextMenu>
                                 <MenuItem Name="Files" Header="Files" IsEnabled="True">
                                    <MenuItem.Icon>
                                       <Image Height="45" Margin=" 0 2 5 0 " Source="{StaticResource PowershellSCript}"/>
                                    </MenuItem.Icon>
                                    <MenuItem Name="ViewFile" Header="View">
                                       <MenuItem.Icon>
                                          <Image Height="45" Margin=" 0 2 5 0 " Source="{StaticResource Service_Detonation}"/>
                                       </MenuItem.Icon>
                                    </MenuItem>
                                    <Separator/>
                                    <MenuItem Name="DownloadF" Header="Download">
                                       <MenuItem.Icon>
                                          <Image Height="45" Margin=" 0 2 5 0 " Source="{StaticResource StorageSync}"/>
                                       </MenuItem.Icon>
                                    </MenuItem>
                                    <Separator/>
                                    <MenuItem Name="RunFile" Header="Run">
                                       <MenuItem.Icon>
                                          <Image Height="45" Margin=" 0 2 5 0 " Source="{StaticResource Service_RTOS}"/>
                                       </MenuItem.Icon>
                                    </MenuItem>
                                 </MenuItem>
                              </ContextMenu>
                           </ListBox.ContextMenu>
                        </ListBox>
                     </ScrollViewer>
                  </Grid>
               </StackPanel>
            </Border>
         </StackPanel>
         <Line
            Width="800"
            Margin="0 20 0 10"
            Stretch="Uniform"
            Stroke="Gainsboro"
            StrokeThickness="1"
            X1="0"
            X2="2"
            Y1="0"
            Y2="0">
         </Line>
         <StackPanel
            HorizontalAlignment="Left"
            Margin="15"
            VerticalAlignment="Top"
            Orientation="Horizontal">
            <Expander
               Name="SObjects"
               Width="400"
               Height="210"
               Margin="10,10,0,10"
               Header="Objects selected"
               IsExpanded="true">
               <StackPanel
                  Height="170"
                  Margin="5,10,5,10"
                  Background="#A7F5F5F5"
                  Orientation="Vertical">
                  <StackPanel Margin="8" Orientation="Horizontal">
                     <Label
                        HorizontalAlignment="Left"
                        Content="Name :"
                        FontSize="13"
                        FontWeight="Bold"/>
                     <Label
                        Name="Name"
                        Width="200"
                        HorizontalAlignment="Left"
                        Content="...."
                        FontSize="13"/>
                  </StackPanel>
                  <StackPanel Margin="8" Orientation="Horizontal">
                     <Label
                        HorizontalAlignment="Left"
                        Content="Last Modified :"
                        FontSize="13"
                        FontWeight="Bold"/>
                     <Label
                        Name="LastModified"
                        Width="150"
                        HorizontalAlignment="Left"
                        Content="...."
                        FontSize="13"/>
                  </StackPanel>
                  <StackPanel Margin="8" Orientation="Horizontal">
                     <Label
                        HorizontalAlignment="Left"
                        Content="URI :"
                        FontSize="13"
                        FontWeight="Bold"/>
                     <Label
                        Name="URL"
                        Width="390"
                        HorizontalAlignment="Left"
                        Content="...."
                        FontSize="11"/>
                  </StackPanel>
                  <StackPanel Margin="8" Orientation="Horizontal">
                     <Label
                        HorizontalAlignment="Left"
                        Content="Sha256 :"
                        FontSize="13"
                        FontWeight="Bold"/>
                     <Label
                        Name="SHA"
                        Width="390"
                        HorizontalAlignment="Left"
                        Content="...."
                        FontSize="11"/>
                  </StackPanel>
               </StackPanel>
            </Expander>
            <Expander
               Name="Content"
               Width="450"
               Height="210"
               Margin="10,10,0,10"
               Header="Content"
               IsExpanded="true">
               <StackPanel
                  Height="250"
                  Margin="5,10,5,10"
                  Background="#A7F5F5F5"
                  Orientation="Vertical">
                  <StackPanel HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Orientation="Horizontal">
                     <TextBox
                        Name="ScriptTextControl"
                        Width="387"
                        Height="165"
                        HorizontalAlignment="Center"
                        VerticalAlignment="Top"
                        AcceptsReturn="True"
                        AcceptsTab="True"
                        Background="Gainsboro"
                        FontFamily="Consolas"
                        FontSize="13"
                        Foreground="Blue"
                        ScrollViewer.HorizontalScrollBarVisibility="Visible"
                        ScrollViewer.VerticalScrollBarVisibility="Visible"
                        Text="#This is the azOSDPad Script PowerShell"
                        TextWrapping="Wrap"/>
                  </StackPanel>
